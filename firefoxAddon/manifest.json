{
    "manifest_version": 2,
    "name": "mengBooru Media Collector",
    "version": "1.0",
    "description": "Collecte des m√©dias depuis les sites web pour les stocker dans votre booru personnel.",
    "icons": {
      "64": "icons/icon_64.png",
      "128": "icons/icon_128.png",
      "514": "icons/icon_514.png"
    },
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus",
      "http://localhost:8080/*",
      "<all_urls>"
    ],
    "browser_action": {
      "default_icon": {
        "64": "icons/icon_64.png"
      },
      "default_title": "mengBooru",
      "default_popup": "popup/popup.html"
    },
    "background": {
      "scripts": ["background/background.js"]
    },
    "web_accessible_resources": [
      "dashboard/dashboard.html",
      "dashboard/dashboard.js",
      "dashboard/dashboard.css"
    ],
    "options_ui": {
      "page": "options/options.html",
      "browser_style": true
    }
  }
  